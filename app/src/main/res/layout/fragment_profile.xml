    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_phone_top"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.385"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_phone_bottom"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.416"
        />

    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_address_top"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.429"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_address_bottom"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.46"
        />

    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_introduction_top"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.485"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_introduction_bottom"
        android:orientation="horizontal"
