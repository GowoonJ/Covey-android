        android:layout_width="0dp"
        android:id="@+id/gd_profile_title_top"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.041"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_square_left"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.044"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_square_right"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.956"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_square_top"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.198"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_square_bottom"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.956"
        />

    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_img_left"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.389"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_img_right"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.611"
        />

    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_img_top"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.136"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_img_bottom"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.261"
        />

    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_img_add_left"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.558"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_img_add_top"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.232"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_edit_left"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.856"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_edit_right"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.911"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_edit_top"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.23"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_edit_bottom"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.261"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_edit_2_bottom"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.63"
        />

    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_text_left"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.089"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:id="@+id/gd_profile_text_right"
