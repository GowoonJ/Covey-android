<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/profile_edit_text_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="경력사항"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintHeight_percent="0.07"
        app:layout_constraintStart_toStartOf="@id/guideline_left_1"
        app:layout_constraintTop_toTopOf="@id/guideline_top" />


    <TextView
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/white_two"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="33dp"
        android:text="업체명"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <EditText
        android:id="@+id/profile_edit_name"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginTop="61dp"
        android:background="@drawable/gray_border_square"
        android:gravity="center_vertical"
        android:hint="업체명을 입력하세요"
        android:paddingLeft="16dp"
        android:text=""
        android:textSize="15sp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="121dp"
        android:text="직무"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <EditText
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginTop="149dp"
        android:background="@drawable/gray_border_square"
        android:gravity="center_vertical"
        android:hint="직무를 입력하세요"
        android:paddingLeft="16dp"
        android:textSize="15sp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="209dp"
        android:text="근무기간"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <EditText
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginTop="237dp"
        android:background="@drawable/gray_border_square"
        android:gravity="center_vertical"
        android:hint="숫자만 입력하세요"
        android:paddingLeft="16dp"
        android:textSize="15sp"
        android:inputType="number"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_2"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <Spinner
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginTop="237dp"
        android:background="@drawable/gray_border_square"
        android:overlapAnchor="false"
        android:popupBackground="@drawable/spinner_background"
        android:popupElevation="0dp"
        android:spinnerMode="dropdown"
        android:textColor="#000000"
        android:dropDownVerticalOffset="4dp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_2"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/white_two"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_0"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_0"
        app:layout_constraintBottom_toTopOf="@id/guideline_bottom_2"
        app:layout_constraintTop_toBottomOf="@id/guideline_bottom_1" />

    <RelativeLayout
        android:id="@+id/profile_edit_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/red_square"
        app:layout_constraintBottom_toTopOf="@id/guideline_button_bottom"
        app:layout_constraintTop_toBottomOf="@id/guideline_button_top"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_1">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:background="@android:color/transparent"
            android:gravity="center_horizontal"
            android:text="완료"
            android:textColor="#ffffff"
            android:textSize="18sp" />
    </RelativeLayout>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_left_0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_left_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.044" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_left_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.603" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_right_0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_right_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.956" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_right_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.581" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="0.035"
        android:orientation="horizontal"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bottom_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="357dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bottom_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="1"
        android:orientation="horizontal"
        />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_button_top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.897" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_button_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="0.975"
        android:orientation="horizontal"
        />
</androidx.constraintlayout.widget.ConstraintLayout>
