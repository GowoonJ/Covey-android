<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <TextView
        android:id="@+id/profile_edit_text_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="개인 정보"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@id/guideline_left_1"
        app:layout_constraintTop_toTopOf="@id/guideline_top"
        app:layout_constraintHeight_percent="0.07" />


    <TextView
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/white_two"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이름"
        android:textColor="#000000"
        android:textSize="16sp"
        android:layout_marginTop="33dp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <EditText
        android:id="@+id/profile_edit_name"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginTop="61dp"
        android:background="@drawable/lightgray_square"
        android:gravity="center_vertical"
        android:paddingLeft="16dp"
        android:text=""
        android:textColor="#888888"
        android:textSize="15sp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="성별"
        android:textColor="#000000"
        android:textSize="16sp"
        android:layout_marginTop="121dp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginTop="149dp"
        android:background="@drawable/lightgray_square"
        android:gravity="center_vertical"
        android:paddingLeft="16dp"
        android:text="여"
        android:textColor="#888888"
        android:textSize="15sp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_2"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="나이"
        android:textColor="#000000"
        android:textSize="16sp"
        android:layout_marginTop="121dp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_2"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginTop="149dp"
        android:background="@drawable/lightgray_square"
        android:gravity="center_vertical"
        android:paddingLeft="16dp"
        android:text="20"
        android:textColor="#888888"
        android:textSize="15sp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_2"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="휴대폰 번호"
        android:textColor="#000000"
        android:textSize="16sp"
        android:layout_marginTop="209dp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginTop="237dp"
        android:background="@drawable/lightgray_square"
        android:gravity="center_vertical"
        android:paddingLeft="16dp"
        android:text="010 1234 5678"
        android:textColor="#888888"
        android:textSize="15sp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="8dp"
        android:layout_marginTop="297dp"
        android:background="@color/white_two"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_0"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_0"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="321dp"
        android:text="주소"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <Spinner
        android:id="@+id/profile_edit_spinner_location"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginTop="349dp"
        android:background="@drawable/gray_border_square"
        android:dropDownVerticalOffset="-4dp"
        android:gravity="center_vertical"
        android:overlapAnchor="false"
        android:popupBackground="@drawable/spinner_background"
        android:popupElevation="0dp"
        android:popupTheme="@style/AppTheme"
        android:spinnerMode="dropdown"
        android:textSize="15sp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_2"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <Spinner
        android:id="@+id/profile_edit_spinner_location_detail"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginTop="349dp"
        android:background="@drawable/gray_border_square"
        android:dropDownVerticalOffset="-4dp"
        android:gravity="center_vertical"
        android:overlapAnchor="false"
        android:popupBackground="@drawable/spinner_background"
        android:popupElevation="0dp"
        android:popupTheme="@style/AppTheme"
        android:spinnerMode="dropdown"
        android:textSize="15sp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_2"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="409dp"
        android:text="한 줄 소개"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <EditText
        android:id="@+id/profile_edit_introduction"
        android:layout_width="0dp"
        android:layout_height="66dp"
        android:layout_marginTop="437dp"
        android:background="@drawable/gray_border_square"
        android:gravity="center_vertical"
        android:paddingLeft="16dp"
        android:textSize="15sp"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="76dp"
        android:layout_marginTop="540dp"
        android:background="@color/white_two"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_0"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_0"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1" />

    <RelativeLayout
        android:id="@+id/profile_edit_button"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginTop="551dp"
        android:background="@drawable/red_square"
        app:layout_constraintLeft_toRightOf="@id/guideline_left_1"
        app:layout_constraintRight_toLeftOf="@id/guideline_right_1"
        app:layout_constraintTop_toBottomOf="@id/profile_edit_text_1">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:background="@android:color/transparent"
            android:gravity="center_horizontal"
            android:text="완료"
            android:textColor="#ffffff"
            android:textSize="18sp" />
    </RelativeLayout>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_left_0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_left_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.044" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_left_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.511" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_right_0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_right_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.956" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_right_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.489" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="0.035"
        android:orientation="horizontal"
        />
</androidx.constraintlayout.widget.ConstraintLayout>
